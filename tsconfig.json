{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@angular.studio/board": [
        "dist/board"
      ],
      "@angular.studio/board-button": [
        "dist/board-button"
      ],
      "@angular.studio/button": [
        "dist/button"
      ],
      "@angular.studio/card": [
        "dist/card"
      ],
      "@angular.studio/dynamic-form": [
        "dist/dynamic-form"
      ],
      "@angular.studio/toolbar": [
        "dist/toolbar"
      ],
      "@angular.studio/dialog": [
        "dist/dialog"
      ],
      "@angular.studio/login": [
        "dist/login"
      ],
      "@angular.studio/toggle": [
        "dist/toggle"
      ],
      "@angular.studio/plans": [
        "dist/plans"
      ],
      "features-table": [
        "dist/features-table/features-table",
        "dist/features-table"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}