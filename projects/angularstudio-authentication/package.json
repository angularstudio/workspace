{
    "name": "@angular.studio/authentication-service",
    "version": "0.0.14",
    "peerDependencies": {
        "@angular/common": "^11.2.9",
        "@angular/core": "^11.2.9",
        "@auth0/angular-jwt": "^5.0.2"
    },
    "devDependencies": {
        "@types/jest": "^26.0.22",
        "@types/node": "^14.14.37",
        "jest": "^26.6.3",
        "ts-jest": "^26.5.4",
        "tslib": "^2.0.0",
        "zone.js": "^0.11.4",
        "uuid": "^8.3.2"
    },
    "jest": {
        "testPathIgnorePatterns": [
            "node_modules/",
            "dist/"
        ],
        "globals": {
            "ts-jest": {
                "tsconfig": "tsconfig.spec.json",
                "stringifyContentPathRegex": "\\.html$"
            }
        },
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest"
        },
        "testMatch": [
            "**/?(*.)+(spec|test).+(ts|tsx|js)"
        ]
    }
}
